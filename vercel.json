{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api_docs/(.*)", "dest": "/api_docs/$1" },
        { "src": "/set-cookie", "dest": "/set-cookie" },
        { "src": "/(.*)", "dest": "/" },
        { "src": "/", "dest": "/"}
    ]
}
